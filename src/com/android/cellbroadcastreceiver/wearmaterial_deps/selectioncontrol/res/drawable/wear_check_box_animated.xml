<?xml version="1.0" encoding="utf-8"?>
<animated-selector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedIds">
  <item
      android:id="@+id/check_box_checked"
      android:drawable="@drawable/ic_wear_check_box_checked"
      android:state_checked="true"
      android:state_enabled="true"/>
  <item
      android:id="@+id/check_box_unchecked"
      android:drawable="@drawable/ic_wear_check_box_unchecked"
      android:state_checked="false"
      android:state_enabled="true"/>
  <item
      android:id="@+id/check_box_checked_disabled"
      android:drawable="@drawable/ic_wear_check_box_checked"
      android:state_checked="true"
      android:state_enabled="false"/>
  <item
      android:id="@+id/check_box_unchecked_disabled"
      android:drawable="@drawable/ic_wear_check_box_unchecked"
      android:state_checked="false"
      android:state_enabled="false"/>
  <transition
      android:drawable="@drawable/wear_check_box_unchecked_to_checked_animation"
      android:fromId="@id/check_box_unchecked"
      android:toId="@id/check_box_checked" />
  <transition
      android:drawable="@drawable/wear_check_box_checked_to_unchecked_animation"
      android:fromId="@id/check_box_checked"
      android:toId="@id/check_box_unchecked" />
  <transition
      android:drawable="@drawable/wear_check_box_disabled_unchecked_to_checked_animation"
      android:fromId="@id/check_box_unchecked_disabled"
      android:toId="@id/check_box_checked_disabled" />
  <transition
      android:drawable="@drawable/wear_check_box_disabled_checked_to_unchecked_animation"
      android:fromId="@id/check_box_checked_disabled"
      android:toId="@id/check_box_unchecked_disabled" />
</animated-selector>
